# Amadeus specific stuff

function amadb() {
    pid=$(ps ux | grep "[o]tf_be.*$2" | tail -1 | awk '{print $2}')
    binary="/data3/gctmp/$USER/myObe/$1/pack/MDW/bin/otf_be"
    echo "Starting gdb with $binary attached to pid $pid"
    gdb "$binary" -p "$pid" -tui
}

lesc () {
    old_colortracer.pl < $* | less -R -R
}
